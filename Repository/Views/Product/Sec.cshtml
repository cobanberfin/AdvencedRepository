@model Repository.Models.ProductModel@{ ViewBag.Title = "Sec";}<h2>Sec</h2><table class="table table-bordered">    <thead>        <tr>            <th>@Html.LabelFor(x => x.Products.ProductID)</th>            <th>@Html.LabelFor(x => x.Products.ProductName)</th>            <th>@Html.LabelFor(x => x.Products.UnitPrice)</th>        </tr>    </thead>    <tbody>        @foreach (var item in Model.plist)        {<tr>    <td>@item.ProductID</td>    <td>@item.ProductName</td>    <td>@item.UnitPrice</td></tr>}    </tbody></table>